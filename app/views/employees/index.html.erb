<div class="well well-sm">
  <span><center>All Employees</center></span>
</div>
<div class="well well-sm">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Emp Id         </th>
        <th>Employee Name  </th>
        <th>Last Name      </th>
        <th>Phone          </th>
        <th>Email          </th>
        <th>Manager Name   </th>
        <th>Joining Date   </th>
        <th>Login          </th>
      </tr>
    </thead>
    <tbody>
      <% @employees.each do |f| %>
        <tr>
          <th><%= link_to f.eid, :controller => "employees", :action => "show", :id => f.id %></th>
          <th><%= link_to f.name, :controller => "employees", :action => "show", :id => f.id %></th>
          <th><%= f.last_name %></th>
          <th><%= f.phone %></th>
          <th><%= f.email %></th>
          <th><%= link_to f.manager.name, :controller => "employees", :action => "show", :id => f.manager.id %></th>
          <th><%= f.j_date %></th>
          <th><%= f.login %></th>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
